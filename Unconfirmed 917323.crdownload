      flipCard(true)
      },300);
      console.log(selectedOption);
      console.log('#######');
      console.log(cardValue);
      console.log('#######');
      if((selectedOption < 7 && cardValue < 7) || (selectedOption > 7 && cardValue > 7) || (selectedOption == cardValue) ){
        if(selectedOption == cardValue && cardValue == 7){
          totalGameScore += 1;
        }
        totalGameScore += 1;
        if(selectedOption == cardValue){
          correctSevenGuessBox.classList.remove('hide')
        }
        else{
          correctGuessBox.classList.remove('hide')
        }
        
        console.log('plus 1')
      } 
      else {
        incorrectGuessBox.classList.remove('hide')
        console.log('minus 1')
        totalGameScore -= 1;
      }
      $(totalPoints).text(totalGameScore);
      
  
    setTimeout(function(){
