function startTimer(){
  var timeleft = 20;
  timerstop = false;
  document.getElementById("countdowntimer").textContent = timeleft;
  var downloadTimer = setInterval(function(){
  timeleft--;
  document.getElementById("countdowntimer").textContent = timeleft;
  if(timeleft <= 0 || timerstop) {
    clearInterval(downloadTimer); 
  }
  if(timeleft == 0){
    cardBox.classList.add("hide"); //show the playboard section
    timeUpBox.classList.remove("hide")
    setTimeout (function(){
      timeUpBox.classList.add("hide")
      gifBox.classList.remove("hide")
    }, 1500);
    
